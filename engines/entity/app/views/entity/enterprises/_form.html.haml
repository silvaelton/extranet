= simple_form_for [@link_enterprise], remote: true do |f|
  .row
    .col-4
      = f.input :entity_id, collection: Address::Enterprise.status_active, include_blank: 'Selecione...'
  %hr.divider
  .row
    .col-12
      = f.button :submit, class: 'btn btn-success', data: { disable_with: t(:disable_with) }