= simple_form_for @requeriment_category, remote: true do |f|
  = f.input :name
  = f.input :description
  = f.input :outside_cadastre, as: :select, include_blank: false
  = f.input :program, as: :select, include_blank: false
  = f.input :program_id, as: :string
  = f.input :status, as: :select, include_blank: false
  %hr.divider
  = f.button :submit, class: 'btn btn-success', data: { disable_with: t(:disable_with)}