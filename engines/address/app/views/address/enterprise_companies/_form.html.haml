= simple_form_for @enterprise_company, remote: true do |f|

  .row
    .col-12 
      = f.input :fantasy_name, as: :string

  .row
    .col-12 
      = f.input :name, as: :string

  .row
    .col-12 
      = f.input :description, as: :string

  .row
    .col-12 
      = f.input :cnpj, as: :string

  .row
    .col-12 
      = f.input :status, as: :string

  .row
    .col-12 
      = f.input :telephone, as: :string

  .row
    .col-12 
      = f.input :email, as: :string

  .row
    .col-12 
      = f.input :address, as: :string

  .row
    .col-12 
      = f.input :city_id, as: :string

  .row
    .col-12 
      = f.input :access_allow, as: :string

  .row
    .col-12 
      = f.input :user_id, as: :string

  .row
    .col-12 
      = f.input :state_id, as: :string

  .row
    .col-12
      = f.button :submit, class: 'btn btn-success', data: { disable_with: t(:disable_with) }